<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    
    <div class="container">

        <div class="row">
            <h2>Configuration</h2>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Filtering</h3>
                </div>
                <div class="panel-body">
                    <div class="form-inline">
                        <label for="input-config-search">Keyword:</label>&nbsp;
                        <input id="input-config-search" class="form-control" type="text" placeholder="Enter search term" />&nbsp;
                        <label for="select-application-config">Application:</label>&nbsp;
                        <select id="select-application-config" class="form-control">
                            <option value="" disabled selected>Select an application</option>
                        </select>&nbsp;
                        <button id="button-clear-search-config" class="btn btn-danger"><span class="fa fa-times"></span></button>
                    </div>
                </div>
            </div>
            <br />
            <table id="table-configuration" class="table table-bordered">
                <thead>
                    <tr>
                        <th>Application</th>
                        <th>Key</th>

                        <th>Edit Quick Values</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</body>
</html>

@section Scripts{
    @Scripts.Render("~/bundles/configuration");
}