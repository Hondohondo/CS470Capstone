<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>SSIS_Configuration</title>
</head>
<body>
    <div class="container">
        <div class="row">
            <h2>SSIS_Configuration</h2>
            <br />
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Filtering</h3>
                </div>
                <div class="panel-body">
                    <div class="form-inline">
                        <label for="select-type">Configured Type:</label>&nbsp;
                        <select id="select-type" class="form-control">
                            <option value="" disabled selected>Select a type</option>
                        </select>&nbsp;
                        <button id="button-clear-search" class="btn btn-danger"><span class="fa fa-times"></span></button>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <table id="table-ssis_configurations" class="table table-bordered">
                    <thead>
                        <tr>
                            <th>ConfigurationFilter</th>
                            <th>ConfiguredValue</th>
                            <th>PackagePath</th>
                            <th>ConfiguredValueType</th>
                        </tr>
                    </thead>
                </table>

            </div>
        </div>
    </div>
</body>
</html>

@section Scripts{
    @Scripts.Render("~/bundles/ssis_configuration");
}
