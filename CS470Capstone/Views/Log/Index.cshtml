<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Logs</title>
</head>
<body>
    <div class="container">
        <div class="row">
            <h2>Logs</h2>
            <br />
            <div class="col-lg-12">
                <table id="table-log" class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Title</th>
                            <th>Severity</th>
                            <th>Priority</th>
                            <th>Timestamp</th>
                            <th>Auth User</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</body>
</html>

@section Scripts{
    @Scripts.Render("~/bundles/log");
}